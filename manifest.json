{
    "name": "#uniswag",
    "version": "1.2",
    "manifest_version": 2,
    "description": "Acceso directo al SIASE pero con swag. @ramomarr",

    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+U",
          "mac": "Command+Shift+U"
        },
        "description": "Abre uniswag."
      }
    },

    "browser_action": {
    	"default_popup": "popup.html",
    	"default_icon": "icon19.png"
    },

     "icons": {
    	"128": "icon128.png",
    	"48": "icon48.png"
    }
}
